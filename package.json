{
  "name": "react-jsonschema",
  "version": "0.4.1",
  "description": "React forms with JSONSchema",
  "main": "lib/index.js",
  "author": {
    "name": "Vu Tran",
    "email": "vu@vu-tran.com",
    "website": "http://vu-tran.com/"
  },
  "bugs": {
    "web": "https://github.com/vutran/react-jsonschema/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vutran/react-jsonschema.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/vutran/react-jsonschema",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "clone": "^1.0.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "jsdom": "^9.4.2",
    "json-loader": "^0.5.4",
    "nyc": "^8.1.0",
    "react-addons-test-utils": "^15.3.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint ./src",
    "compile": "babel ./src -d ./lib",
    "build": "npm run clean && npm run compile",
    "build-docs": "webpack --content-base docs --config docs/webpack.production.config.js",
    "dev": "webpack-dev-server --content-base docs/ --config docs/webpack.config.js --inline --hot",
    "prepublish": "npm run build",
    "test": "nyc ava"
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-tap-event-plugin": "^1.0.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "**/*.js",
      "!lib/**/*"
    ],
    "require": [
      "babel-register"
    ],
    "concurrency": 5,
    "failFast": true,
    "babel": "inherit"
  }
}
